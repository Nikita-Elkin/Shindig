<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_pressed="true"  android:top="2dp" android:bottom="2dp">
    <shape android:shape="rectangle">
        <corners
            android:radius="5dp" />
        <solid android:color="@color/button_clicked"/>
    </shape>
</item>
<item>
    <shape android:shape="rectangle">
        <corners
            android:radius="5dp" />
        <stroke
            android:width="2dp"
            android:color="@color/white"/>
    </shape>
</item>
</selector>
